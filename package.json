{
  "name": "caterpillar",
  "version": "0.0.1",
  "description": "Clone of Snake game made in TerminalTXT.",
  "scripts": {
    "start": "parcel server ./src/index.html",
    "404": "parcel ./src/404.html",
    "build": "rm -rf dist && parcel build ./src/index.html ./src/404.html --public-url /caterpillar",
    "deploy": "gh-pages -d ./dist",
    "build-deploy": "npm run build && npm run deploy"
  },
  "author": "Fi Graham",
  "license": "MIT",
  "dependencies": {
    "terminaltxt": "0.0.5"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "parcel": "^1.12.3",
    "typescript": "^3.4.5"
  }
}
